{
  "name": "worka",
  "version": "6.0.2",
  "description": "Easy web workers modelled as HTTP API endpoints",
  "main": "built/worka_require.js",
  "module": "source/worka.js",
  "directories": {
    "example": "example"
  },
  "scripts": {
    "test": "echo See examples",
    "buildExample": "rollup example/estimatePi.js --output.format cjs --output.file example/estimatePiNode.js",
    "buildBrowser": "rollup source/worka.js --output.format iife --output.name \"worka\" --output.file built/worka_script.js",
    "buildCommonJs": "rollup source/worka.js --output.format cjs --output.name \"worka\" --output.file built/worka_require.js",
    "prepublishOnly": "npm run buildBrowser && npm run buildCommonJs",
    "try": "node example/index.js 8080",
    "tryNode": "node example-node/simplest_usage.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/GrosSacASac/worka.git"
  },
  "author": "GrosSacASacs",
  "license": "BSL-1.0",
  "devDependencies": {
    "rollup": "^1.1.2"
  },
  "keywords": [
    "web-worker",
    "promise"
  ]
}
